class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int n = nums.size();
        int i = 0;

        while(i <= n){
            for(int j=0; j<n; j++){
                if( i == nums[j]){
                    i++;
                    break;
                }
                else if(i != nums[j] && j == n-1){
                    return i;
                }
            }
        }
        return -1;
    }
};
