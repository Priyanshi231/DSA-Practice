class Solution {
public:
    int findKthPositive(vector<int>& nums, int k) {
        int n = nums.size();
   
        int s = 0;
        int e = nums.size()-1;

        while(s<=e){
            int m = s+(e-s)/2;

            int missing = nums[m] - (m+1);

            if(missing < k){
                s = m+1;
            }else{
                e = m-1;
            }
        }
        return s + k;
    } 
};
