class Solution {
public:
    void rotateArray(vector<int>& nums, int k) {
        int n = nums.size();
        int count = 0;
        int i=0;

        if(k>n){
            k = k%n;
        }
        
        while(count != k){
            int temp = nums[i];

            for(int j=1; j<n; j++){
                nums[j-1] = nums[j];
            }
            nums[n-1] = temp;
            count++;
        }
    }
};
