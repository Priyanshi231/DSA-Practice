class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int n  = nums.size();
        int i=0;
        int ans = 0;

        while(i<n){
            int c = 0;

            while(i<n && nums[i] ==1){
                c++;
                i++;
            }
            ans = max(ans,c);
            i++;
        }
        return ans;
    }
};
