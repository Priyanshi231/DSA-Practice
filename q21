class Solution {
public:
    vector<int> sort(vector<int>& nums, int n){
        if(n == 1) return nums;

        for(int i=0; i<n-1; i++){
                if(nums[i] > nums[i+1]){
                    swap(nums[i],nums[i+1]);
                }
        }
        return sort(nums,n-1);

    }
    vector<int> bubbleSort(vector<int>& nums) {
        int n = nums.size();
        return sort(nums,n);
    }
};
