class Solution{
public:
    int upperBound(vector<int> &nums, int x){
        int n = nums.size();
        int s= 0;
        int e = n-1;
        int ans =n;

        while(s<=e){
            int m = s+(e-s)/2;

            if(nums[m] > x){
                ans = m;
                e = m-1;
            }

            else{
                s = m+1;
            }
        }
        return ans;
    }
};
