class Solution {
public:
    vector<int> leaders(vector<int>& nums) {
       int n = nums.size();

       vector<int> ans;

       int m = nums[n-1];
       ans.push_back(m);

       for(int i=n-2; i>=0; i--){
            if(nums[i]>m){
                ans.push_back(nums[i]);
                m = nums[i];
            }
       }
       reverse(ans.begin(),ans.end());
       return ans;
    }
};
