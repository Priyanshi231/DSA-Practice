class Solution {
public:
    int mostFrequentElement(vector<int>& nums) {
        unordered_map<int,int> m;
        int maxfreq = 0;
        int res = -1;

        for(int num : nums){
            m[num]++;
            int freq = m[num];
            if(freq > maxfreq || (freq == maxfreq && res > num)){
                maxfreq = freq;
                res = num;
            }
        }
        return res;
    }
};
