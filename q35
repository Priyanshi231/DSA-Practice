class Solution{    
public:    
    int singleNumber(vector<int>& nums){
        int n = nums.size();

        if(n==1){
            return nums[0];
        }
        
        for(int i=0; i<n; i++){
            for(int j=0; j<n; j++){
                if(i != j && nums[i] == nums[j]){
                    break;
                }
                else if (j == n-1 && nums[i] != nums[j]){
                    return nums[i];
                }
            }
        }
        return -1;
    }
};
